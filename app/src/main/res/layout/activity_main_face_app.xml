<?xml version="1.0" encoding="utf-8"?>


<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawerLayout_faceapp_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_Face_App">


    <LinearLayout xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/LinearLayout_faceapp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F5FFFA"
        android:foregroundGravity="center"
        android:scrollbars="vertical"
        android:orientation="vertical"
        tools:context=".MainActivity_Face_App">

        <ImageView
            android:id="@+id/imageView_ЗначекApp"
            android:layout_width="37dp"
            android:layout_height="37dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="10dp"
            android:foregroundGravity="center_vertical"
            android:scaleType="centerCrop"
        android:stateListAnimator="@android:animator/fade_in"
        android:visibility="visible"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/icon_dsu1_for_mains_menu_faceapp111"
        tools:src="@tools:sample/avatars" />



    <ScrollView
        android:id="@+id/ScrollFaceApp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView_ЗначекApp">

        <LinearLayout
            android:id="@+id/LineLayFaceApp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardview1_For_MainActivity"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:layout_weight="2"
                android:minHeight="190dp"
                android:animateLayoutChanges="true"
                android:checkable="true"
                android:clickable="true"
                android:focusable="true"
                android:singleLine="false"
                android:textSize="10sp"
                app:cardBackgroundColor="#F0FFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="3dp"
                app:cardMaxElevation="4dp"
                app:strokeColor="#00ACC1"
                android:translationZ="10dp"
                app:strokeWidth="2dp">

                <RelativeLayout
                    android:id="@+id/relativelayoutcardviewmatirealMainActityt1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="2dp"
                    android:paddingTop="2dp"
                    android:paddingRight="2dp"
                    android:paddingBottom="2dp">


                    <TextView
                        android:id="@+id/text1_inner_ardview_forMainActivity"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:checkMark="?android:attr/textCheckMark"
                        android:foregroundGravity="center"
                        android:gravity="center_horizontal|center_vertical"
                        android:minLines="5"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:singleLine="false"
                        android:text="Задачи"
                        android:textAlignment="gravity"
                        android:textAppearance="?android:attr/textAppearanceListItemSmall"
                        android:textColor="#000000"
                        android:textSize="19sp"
                        android:textStyle="bold"
                        android:visibility="visible" />

                    <ProgressBar
                        android:id="@+id/prograessbarTask_inner_ardview_forMainActivity"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:indeterminate="true"
                        android:visibility="invisible" />


                    <ImageView
                        android:id="@+id/imageview_cardview_for_tabel_1"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_alignLeft="@+id/text1_inner_ardview_forMainActivity"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        app:srcCompat="@drawable/icon_dsu1_for_mainactivity_tasks1" />


                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardview2_For_MainActivity"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:layout_weight="2"
                android:minHeight="190dp"
                android:animateLayoutChanges="true"
                android:checkable="true"
                android:clickable="true"
                android:focusable="true"
                android:singleLine="false"
                android:textSize="10sp"
                app:cardBackgroundColor="#F0FFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="3dp"
                app:cardMaxElevation="4dp"
                app:strokeColor="#00ACC1"
                android:translationZ="10dp"
                app:strokeWidth="2dp">

                <RelativeLayout
                    android:id="@+id/relativelayoutcardviewmatirealMainActityt2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="2dp"
                    android:paddingTop="2dp"
                    android:paddingRight="2dp"
                    android:paddingBottom="2dp">

                    <TextView
                        android:id="@+id/text2_inner_ardview_forMainActivity"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:checkMark="?android:attr/textCheckMark"
                        android:foregroundGravity="center"
                        android:gravity="center_horizontal|center_vertical"
                        android:minLines="5"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:singleLine="false"
                        android:text="Табель"
                        android:textAlignment="gravity"
                        android:textAppearance="?android:attr/textAppearanceListItemSmall"
                        android:textColor="#000000"
                        android:textSize="19sp"
                        android:textStyle="bold"
                        android:visibility="visible" />

                    <ProgressBar
                        android:id="@+id/prograessbarTabel_inner_ardview_forMainActivity"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:indeterminate="true"
                        android:visibility="invisible" />

                    <ImageView
                        android:id="@+id/imageview_cardview_for_tabel_2"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_alignLeft="@+id/text2_inner_ardview_forMainActivity"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        app:srcCompat="@drawable/icon_dsu1_for_mainactivity_tabel2" />


                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/cardview3_For_MainActivity"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:layout_weight="2"
                android:animateLayoutChanges="true"
                android:checkable="true"
                android:clickable="true"
                android:minHeight="190dp"
                android:focusable="true"
                android:singleLine="false"
                android:textSize="10sp"
                app:cardBackgroundColor="#F0FFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="3dp"
                app:cardMaxElevation="4dp"
                app:strokeColor="#00ACC1"
                android:translationZ="10dp"
                app:strokeWidth="2dp">

                <RelativeLayout
                    android:id="@+id/relativelayoutcardviewmatirealMainActityt3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="2dp"
                    android:paddingTop="2dp"
                    android:paddingRight="2dp"
                    android:paddingBottom="2dp">

                    <TextView
                        android:id="@+id/text3_inner_ardview_forMainActivity"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:checkMark="?android:attr/textCheckMark"
                        android:foregroundGravity="center"
                        android:gravity="center_horizontal|center_vertical"
                        android:minLines="5"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:paddingRight="10dp"
                        android:singleLine="false"
                        android:text="Чат"
                        android:textAlignment="gravity"
                        android:textAppearance="?android:attr/textAppearanceListItemSmall"
                        android:textColor="#000000"
                        android:textSize="19sp"
                        android:textStyle="bold"
                        android:visibility="visible" />

                    <ProgressBar
                        android:id="@+id/prograessbarChats_inner_ardview_forMainActivity"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:indeterminate="true"
                        android:visibility="invisible" />

                    <ImageView
                        android:id="@+id/imageview_cardview_for_tabel_3"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_alignLeft="@+id/text3_inner_ardview_forMainActivity"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        app:srcCompat="@drawable/icon_dsu1_for_mainactivity_chats3" />

                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>


        </LinearLayout>
    </ScrollView>

        <com.google.android.material.navigation.NavigationView
            android:id="@+id/navigationview_faceapp"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            tools:context=".MainActivity_Face_App"
            app:menu="@menu/menu_for_faceapp_navigationview"
            tools:ignore="MissingConstraints"></ com.google.android.material.navigation.NavigationView>


    </LinearLayout>

</androidx.drawerlayout.widget.DrawerLayout>